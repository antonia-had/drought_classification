   "id": "53d9cedf",
   "metadata": {
    "pycharm": {
     "name": "#%% md\n"
    }
   },
   "source": [
    "Define function that identifies the percentile violated"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "d47a6591",
   "metadata": {
    "pycharm": {
     "name": "#%%\n"
    }
   },
   "outputs": [],
   "source": [
    "def percentile(x):\n",
    "    for i, per in enumerate(deliveries_thresholds):\n",
    "        if x<= per:\n",
    "            violation = i\n",
    "            break\n",
    "        else:\n",
    "            violation = 999\n",
    "    return violation"
   ]
